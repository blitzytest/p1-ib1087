{
  "name": "mint-clone-mobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace MintClone.xcworkspace -scheme MintClone -configuration Release",
    "security-audit": "npm audit",
    "clean": "rm -rf build dist",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.0",
    "@react-native-firebase/analytics": "^18.0.0",
    "@react-native-firebase/messaging": "^16.0.0",
    "@react-navigation/bottom-tabs": "^6.0.0",
    "@react-navigation/native": "^6.0.0",
    "@react-navigation/stack": "^6.0.0",
    "@reduxjs/toolkit": "^1.9.0",
    "@sentry/react-native": "^5.0.0",
    "@ui-kitten/components": "^5.1.0",
    "axios": "^1.3.0",
    "axios-cache-adapter": "^2.7.3",
    "axios-rate-limit": "^1.3.0",
    "axios-retry": "^3.5.0",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "intl": "^1.2.0",
    "lodash": "^4.17.21",
    "memory-cache": "^0.2.0",
    "mixpanel-react-native": "^2.3.0",
    "opossum": "^6.0.0",
    "react": "18.2.0",
    "react-error-boundary": "^4.0.0",
    "react-native": "0.71.0",
    "react-native-biometrics": "^3.0.1",
    "react-native-device-info": "^8.7.1",
    "react-native-encrypted-storage": "4.0.3",
    "react-native-fingerprint-scanner": "6.0.0",
    "react-native-get-random-values": "^1.8.0",
    "react-native-plaid-link-sdk": "10.0.0",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-svg": "13.0.0",
    "react-native-touch-id": "^4.4.1",
    "react-native-vector-icons": "^9.0.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-state-sync": "^3.1.0",
    "styled-components": "^5.3.0",
    "validator": "^13.0.0",
    "victory-native": "36.0.0",
    "winston": "^3.8.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-decorators": "^7.22.0",
    "@babel/plugin-transform-runtime": "^7.22.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.0",
    "@jest/globals": "^29.0.0",
    "@jest/types": "^29.0.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.0.0",
    "@testing-library/react": "^13.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^16.0.0",
    "@types/react": "^18.0.0",
    "@types/react-native": "0.71.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "babel-jest": "^29.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.27.0",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "jest-fetch-mock": "^3.0.0",
    "jest-performance": "^1.0.0",
    "metro-config": "^0.76.5",
    "metro-react-native-babel-preset": "^0.76.5",
    "prettier": "^2.8.0",
    "typescript": "^4.9.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "setupFiles": [
      "./test/setup.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}